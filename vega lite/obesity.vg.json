{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "width": 800,
  "height": 800,
  "title": "Adults Obesity ASR in Victoria",
  "data": {
    "url": "https://raw.githubusercontent.com/ahilaaa/fit3179/refs/heads/main/data/topojson/victoria.json",
    "format": {
      "type": "topojson",
      "feature": "suburb-2-vic"
    }
  },
  "transform": [
    {
      "lookup": "properties.vic_loca_2",
      "from": {
        "data": {
          "url": "path/to/Obesity Council.csv",
          "format": {"type": "csv"}
        },
        "key": "suburb",
        "fields": ["Adults - Obesity - ASR"]
      }
    }
  ],
  "params": [
    {
      "name": "zoomLevel",
      "value": 5000,
      "bind": {
        "input": "range",
        "min": 5000,
        "max": 100000,
        "step": 100,
        "name": "zoom"
      }
    }
  ],
  "projection": {
    "type": "mercator",
    "scale": {"expr": "zoomLevel"},
    "center": [144.9631, -37.8136]
  },
  "mark": {
    "type": "geoshape",
    "stroke": null
  },
  "encoding": {
    "color": {
      "condition": {
        "test": "datum['Adults - Obesity - ASR'] == null",
        "value": "lightgrey"
      },
      "field": "Adults - Obesity - ASR",
      "type": "quantitative",
      "scale": {"scheme": "reds"},
      "legend": {"title": "Obesity ASR"}
    },
    "tooltip": [
      {"field": "properties.vic_loca_2", "type": "nominal", "title": "Suburb"},
      {"field": "Adults - Obesity - ASR", "type": "quantitative", "title": "Obesity ASR"}
    ]
  }
}

