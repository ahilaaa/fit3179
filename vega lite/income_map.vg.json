{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "width": 1200,
    "height": 800,
    "title": "Median Income Victoria",
    "projection": { "type": "mercator" },
    "layer": [
      {
        "data": {
          "url": "../data/topojson/victoria.json",
          "format": {
            "type": "topojson",
            "feature": "suburb-2-vic"
          }
        },
        "mark": {
          "type": "geoshape",
          "stroke": "black",
          "fill": "lightgray",
          "strokeWidth": 0.5
        },
        "encoding": {
          "tooltip": [
            { "field": "properties.vic_loca_2", "type": "nominal", "title": "Suburb" }
          ]
        }
      },
      {
        "data": {
          "url": "../data/topojson/victoria.json",
          "format": {
            "type": "topojson",
            "feature": "suburb-2-vic"
          }
        },
        "transform": [
          {
            "lookup": "properties.vic_loca_2",
            "from": {
              "data": {
                "url": "../data/csv/Income VIC.csv"
              },
              "key": "suburb",
              "fields": ["Median3 taxable income 2021-22$"]
            }
          },
          {
            "calculate": "toNumber(replace(replace(datum['Median3 taxable income 2021-22$'], ',', ''), '\"', ''))",
            "as": "median_income_21"
          }
        ],
        "mark": { "type": "geoshape" },
        "encoding": {
          "color": {
            "field": "median_income_21",
            "type": "quantitative",
            "scale": { "scheme": "blues" }
          },
          "tooltip": [
            { "field": "properties.vic_loca_2", "type": "nominal", "title": "Suburb" },
            { "field": "median_income_21", "type": "quantitative", "title": "Median Income" }
          ]
        }
      }
    ]
  }
  