{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "data": {
      "url": "https://raw.githubusercontent.com/ahilaaa/fit3179/refs/heads/main/data/csv/Income%20VIC.csv"
    },
    "transform": [
      {
        "lookup": "Suburb",
        "from": {
          "data": {
            "url": "https://raw.githubusercontent.com/ahilaaa/fit3179/refs/heads/main/data/csv/KFC%20Locations.csv"
          },
          "key": "Suburb",
          "fields": ["FastFoodCount"]
        }
      },
      {
        "calculate": "datum.FastFoodCount / datum.Population",
        "as": "FastFoodPerCapita"
      }
    ],
    "layer": [
      {
        "mark": "bar",
        "encoding": {
          "x": {
            "field": "Suburb",
            "type": "nominal",
            "axis": {"title": "Suburb", "labelAngle": -45}
          },
          "y": {
            "field": "FastFoodPerCapita",
            "type": "quantitative",
            "axis": {"title": "Fast Food Per Capita"},
            "scale": {"domain": [0, 7]}  
          },
          "color": {
            "value": "#1f77b4"
          }
        }
      },
      {
        "mark": "bar",
        "encoding": {
          "x": {
            "field": "Suburb",
            "type": "nominal"
          },
          "y": {
            "field": "MedianIncome",
            "type": "quantitative",
            "axis": {"title": "Median Income"},
            "scale": {"domain": [0, 100000]}  
          },
          "color": {
            "value": "#ff7f0e"
          }
        }
      }
    ],
    "resolve": {
      "scale": {"y": "independent"}
    }
  }
  