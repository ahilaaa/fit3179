{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "description": "Fast Food Chains per Capita vs. Median Income Bar Chart",
  "hconcat": [
    {
      "data": {
        "url": "https://raw.githubusercontent.com/ahilaaa/fit3179/refs/heads/main/data/csv/KFC%20Locations.csv",
        "format": {"type": "csv"}
      },
      "mark": "bar",
      "encoding": {
        "x": {
          "field": "Suburb",
          "type": "nominal",
          "axis": {"title": "Suburb", "labelAngle": -45},
          "sort": "ascending"
        },
        "y": {
          "aggregate": "count",
          "field": "KFC LocationName",
          "type": "quantitative",
          "axis": {"title": "Number of KFC Outlets"}
        },
        "tooltip": [
          {"field": "Suburb", "type": "nominal", "title": "Suburb"},
          {
            "aggregate": "count",
            "field": "KFC LocationName",
            "type": "quantitative",
            "title": "Number of KFC Outlets"
          }
        ]
      },
      "title": "Number of KFC Outlets per Suburb"
    },
    {
      "data": {
        "url": "https://raw.githubusercontent.com/ahilaaa/fit3179/refs/heads/main/data/csv/Income%20VIC.csv",
        "format": {"type": "csv"}
      },
      "mark": "bar",
      "encoding": {
        "x": {
          "field": "suburb",
          "type": "nominal",
          "axis": {"title": "Suburb", "labelAngle": -45},
          "sort": "ascending"
        },
        "y": {
          "field": "Median taxable income 2021-22",
          "type": "quantitative",
          "axis": {"title": "Median Taxable Income"}
        },
        "tooltip": [
          {"field": "suburb", "type": "nominal", "title": "Suburb"},
          {
            "field": "Median taxable income 2021-22",
            "type": "quantitative",
            "title": "Median Taxable Income"
          }
        ]
      },
      "title": "Median Taxable Income per Suburb"
    }
  ]
}
